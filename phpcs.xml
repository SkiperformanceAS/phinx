<?xml version="1.0"?>
<ruleset name="Phinx">
    <config name="installed_paths" value="../../cakephp/cakephp-codesniffer"/>

    <rule ref="CakePHP"/>

    <rule ref="SlevomatCodingStandard.TypeHints.DeclareStrictTypes">
        <severity>0</severity>
    </rule>

    <exclude-pattern>*_files*</exclude-pattern>
</ruleset>
